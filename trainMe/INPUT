<div id="listing">

<div class="boom-line-header">
    <h1>Muscle Listing &ndash; Exercise Guides</h1>
</div>

<div class="search">
	
<div class="searchBoxExercisesCon" id="searchBoxExercisesCon">
    <div class="viewAllExercises"style="display: none">
        <h3><a href="/exercises/list/">View All Exercises</a></h3>
    </div>
    <span class="delimiter">|</span>
    <div class="searchBoxExercises">
        <h4>Search</h4><h4 class="mobile-only">Or Search</h4> <input type="text" name="exerciseNameSearch" id="exerciseNameSearch" value="" onblur="SetText();" onfocus="ClearText();"/>
    </div>
</div>

<script type="text/javascript">
    var exerciseDataSource = 0;
    var autoCompleteVal  = "";
    function sh(n, s) { $(n).css("visibility", s); }

    $(function() {

        // Keypress event on the #name. Adjust other form elements based on text
        $("input#exerciseNameSearch").keypress(function(e) {
            if ($("input#exerciseNameSearch").val() != autoCompleteVal)
                exerciseDataSource = 0;
        });

        // Add auto complete functionality to the "name" element
        $('#exerciseNameSearch').bbautocomplete('http://www.bodybuilding.com/exercises/main/getmatchingexercisenames/', {
			delay: 100,
            minChars: 2,
            scroll: true,
            scrollHeight:200,
            matchContains:true, // very important, makes "Ab Ro" pull up "Ab Roller",
            showResultCount:true,
            showResultCountMessage:'Click exercise name for details.',
            extraParams: {
                exerciseNameSearch: function(){ return $('#exerciseNameSearch').val(); }
            }
        }).result(function(event, data, formatted) {
            $("#exerciseNameSearch").val(formatted);
            autoCompleteVal = formatted;
            exerciseDataSource = 1;
            return false;
        }).blur(function(){
            $("#exerciseNameSearch").flushCache();
        }).result(function(event, item) {
            var url = 'http://www.bodybuilding.com/exercises/detail/view/name/' + item[1];
            location.href = url;
        });

    });
    function ClearText(){
        var exerciseNameSearch = document.getElementById('exerciseNameSearch');
        if (exerciseNameSearch.value == ""){
            exerciseNameSearch.value = '';
        }

    }
   function SetText(){
        var exerciseNameSearch = document.getElementById('exerciseNameSearch');
        if (exerciseNameSearch.value == ''){
            exerciseNameSearch.value = '';
        }
    }
</script>


